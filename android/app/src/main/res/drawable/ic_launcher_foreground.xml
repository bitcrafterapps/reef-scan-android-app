<vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">

    <!-- BASE LAYER: Deep Shadow/Depth -->
    <!-- Creates the dark voids between coral branches -->
    <path android:pathData="M20,20 Q54,10 88,20 Q98,54 88,88 Q54,98 20,88 Q10,54 20,20 Z">
        <aapt:attr name="android:fillColor">
            <gradient android:type="radial" android:centerX="54" android:centerY="54" android:gradientRadius="50">
                <item android:color="#FF1A0535" android:offset="0.0"/> <!-- Deep Indigo Center -->
                <item android:color="#00000000" android:offset="1.0"/> <!-- Transparent Edge -->
            </gradient>
        </aapt:attr>
    </path>

    <!-- LAYER 1: Background Tentacles (Out of Focus) -->
    <!-- Blurred, darker purple tentacles in the back -->
    <path android:pathData="M30,35 Q35,25 40,35 T50,35 T60,30 T70,40 T80,35 T75,50 T80,60 T75,70 T65,75 T55,70 T45,75 T35,70 T30,60 T25,50 Z"
          android:fillAlpha="0.6">
        <aapt:attr name="android:fillColor">
            <gradient android:type="linear" android:startX="30" android:startY="30" android:endX="80" android:endY="80">
                <item android:color="#FF4A148C" android:offset="0.0"/> <!-- Dark Purple -->
                <item android:color="#FF311B92" android:offset="1.0"/> <!-- Deep Blue -->
            </gradient>
        </aapt:attr>
    </path>

    <!-- LAYER 2: Mid-Ground Tentacles (Semi-Transparent) -->
    <!-- Simulating the fleshy body of Euphyllia -->
    <!-- Group 1: Left Cluster -->
    <path android:pathData="M25,55 Q25,40 35,45 T45,35 T55,45 T45,55 T35,65 Z">
        <aapt:attr name="android:fillColor">
            <gradient android:type="radial" android:centerX="40" android:centerY="50" android:gradientRadius="25">
                <item android:color="#AA7B1FA2" android:offset="0.0"/> <!-- Translucent Purple -->
                <item android:color="#004A148C" android:offset="1.0"/> <!-- Fade to clear -->
            </gradient>
        </aapt:attr>
    </path>
    
    <!-- Group 2: Right Cluster -->
    <path android:pathData="M65,45 Q75,35 85,45 T75,65 T65,75 T55,65 Z">
        <aapt:attr name="android:fillColor">
            <gradient android:type="radial" android:centerX="70" android:centerY="55" android:gradientRadius="25">
                <item android:color="#AA7B1FA2" android:offset="0.0"/>
                <item android:color="#004A148C" android:offset="1.0"/>
            </gradient>
        </aapt:attr>
    </path>

    <!-- LAYER 3: Foreground "Torch" Tips (The Glowing Heads) -->
    <!-- These are the realistic focal points. Using gradients to simulate internal bioluminescence -->

    <!-- Tip 1: Top Left -->
    <path android:pathData="M35,38 C32,38 30,41 32,44 C34,47 38,47 40,44 C42,41 40,38 37,38 Z">
        <aapt:attr name="android:fillColor">
            <gradient android:type="radial" android:centerX="36" android:centerY="41" android:gradientRadius="6">
                <item android:color="#FFFFEB3B" android:offset="0.0"/> <!-- Bright Yellow Core -->
                <item android:color="#FFC6FF00" android:offset="0.5"/> <!-- Neon Green Halo -->
                <item android:color="#0064DD17" android:offset="1.0"/> <!-- Fade out -->
            </gradient>
        </aapt:attr>
    </path>

    <!-- Tip 2: Top Right -->
    <path android:pathData="M70,35 C67,35 65,38 67,41 C69,44 73,44 75,41 C77,38 75,35 72,35 Z">
        <aapt:attr name="android:fillColor">
            <gradient android:type="radial" android:centerX="71" android:centerY="38" android:gradientRadius="6">
                <item android:color="#FFFFEB3B" android:offset="0.0"/>
                <item android:color="#FFC6FF00" android:offset="0.5"/>
                <item android:color="#0064DD17" android:offset="1.0"/>
            </gradient>
        </aapt:attr>
    </path>

    <!-- Tip 3: Center Main (Largest) -->
    <path android:pathData="M50,50 C46,50 44,54 47,58 C50,62 58,62 61,58 C64,54 62,50 58,50 Z">
        <aapt:attr name="android:fillColor">
            <gradient android:type="radial" android:centerX="54" android:centerY="54" android:gradientRadius="10">
                <item android:color="#FFFFFF00" android:offset="0.0"/> <!-- Super Bright Gold -->
                <item android:color="#FF76FF03" android:offset="0.6"/> <!-- Neon Green -->
                <item android:color="#001B5E20" android:offset="1.0"/> <!-- Dark Fade -->
            </gradient>
        </aapt:attr>
    </path>

    <!-- Tip 4: Bottom Left -->
    <path android:pathData="M30,65 C28,65 26,68 28,71 C30,74 34,74 36,71 C38,68 36,65 33,65 Z">
        <aapt:attr name="android:fillColor">
            <gradient android:type="radial" android:centerX="32" android:centerY="68" android:gradientRadius="6">
                <item android:color="#FFCCFF90" android:offset="0.0"/>
                <item android:color="#FF64DD17" android:offset="0.6"/>
                <item android:color="#001B5E20" android:offset="1.0"/>
            </gradient>
        </aapt:attr>
    </path>

    <!-- Tip 5: Bottom Right -->
    <path android:pathData="M75,60 C72,60 70,63 72,66 C74,69 78,69 80,66 C82,63 80,60 77,60 Z">
        <aapt:attr name="android:fillColor">
            <gradient android:type="radial" android:centerX="76" android:centerY="63" android:gradientRadius="6">
                <item android:color="#FFCCFF90" android:offset="0.0"/>
                <item android:color="#FF64DD17" android:offset="0.6"/>
                <item android:color="#001B5E20" android:offset="1.0"/>
            </gradient>
        </aapt:attr>
    </path>

    <!-- LAYER 4: Highlights / Caustics -->
    <!-- Subtle white light streaks hitting the fleshy parts to look "wet" -->
    <path android:pathData="M45,40 Q50,35 55,40" android:strokeWidth="1" android:strokeColor="#40FFFFFF" android:strokeLineCap="round"/>
    <path android:pathData="M60,45 Q65,40 70,45" android:strokeWidth="1" android:strokeColor="#40FFFFFF" android:strokeLineCap="round"/>
    <path android:pathData="M30,50 Q35,45 40,50" android:strokeWidth="1" android:strokeColor="#30FFFFFF" android:strokeLineCap="round"/>

</vector>
