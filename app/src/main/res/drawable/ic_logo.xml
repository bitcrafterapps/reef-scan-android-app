<vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">

    <!-- Macro Euphyllia Coral Design for Splash Screen -->
    
    <!-- Background Tentacle Mass (Deep Purple/Blue Base) -->
    <path android:pathData="M20,30 Q35,10 65,20 Q90,25 85,55 Q80,85 50,90 Q20,85 15,55 Q10,35 20,30 Z"
          android:fillAlpha="0.8">
        <aapt:attr name="android:fillColor">
            <gradient android:type="radial" android:centerX="54" android:centerY="54" android:gradientRadius="50">
                <item android:color="#FF311B92" android:offset="0.0"/> <!-- Deep Indigo -->
                <item android:color="#00000000" android:offset="1.0"/> <!-- Fade out -->
            </gradient>
        </aapt:attr>
    </path>

    <!-- Glowing Torch Tips - Radiating out from center -->
    
    <!-- Tip 1: Top Center -->
    <path android:pathData="M50,25 C48,25 46,28 48,31 C50,34 54,34 56,31 C58,28 56,25 54,25 Z">
        <aapt:attr name="android:fillColor">
            <gradient android:type="radial" android:centerX="52" android:centerY="28" android:gradientRadius="8">
                <item android:color="#FFFFEB3B" android:offset="0.0"/> <!-- Gold -->
                <item android:color="#FF76FF03" android:offset="0.7"/> <!-- Neon Green -->
                <item android:color="#001B5E20" android:offset="1.0"/>
            </gradient>
        </aapt:attr>
    </path>

    <!-- Tip 2: Top Left -->
    <path android:pathData="M30,35 C28,35 26,38 28,41 C30,44 34,44 36,41 C38,38 36,35 33,35 Z">
        <aapt:attr name="android:fillColor">
            <gradient android:type="radial" android:centerX="32" android:centerY="38" android:gradientRadius="8">
                <item android:color="#FFC6FF00" android:offset="0.0"/>
                <item android:color="#FF64DD17" android:offset="0.7"/>
                <item android:color="#001B5E20" android:offset="1.0"/>
            </gradient>
        </aapt:attr>
    </path>

    <!-- Tip 3: Top Right -->
    <path android:pathData="M75,38 C73,38 71,41 73,44 C75,47 79,47 81,44 C83,41 81,38 78,38 Z">
        <aapt:attr name="android:fillColor">
            <gradient android:type="radial" android:centerX="77" android:centerY="41" android:gradientRadius="8">
                <item android:color="#FFC6FF00" android:offset="0.0"/>
                <item android:color="#FF64DD17" android:offset="0.7"/>
                <item android:color="#001B5E20" android:offset="1.0"/>
            </gradient>
        </aapt:attr>
    </path>

    <!-- Tip 4: Center (Main Focus) -->
    <path android:pathData="M48,50 C44,50 42,55 45,60 C48,65 56,65 59,60 C62,55 60,50 56,50 Z">
        <aapt:attr name="android:fillColor">
            <gradient android:type="radial" android:centerX="52" android:centerY="55" android:gradientRadius="12">
                <item android:color="#FFFFFF00" android:offset="0.0"/> <!-- Bright Gold -->
                <item android:color="#FF76FF03" android:offset="0.6"/>
                <item android:color="#0033691E" android:offset="1.0"/>
            </gradient>
        </aapt:attr>
    </path>

    <!-- Tip 5: Bottom Left -->
    <path android:pathData="M35,65 C33,65 31,68 33,71 C35,74 39,74 41,71 C43,68 41,65 38,65 Z">
        <aapt:attr name="android:fillColor">
            <gradient android:type="radial" android:centerX="37" android:centerY="68" android:gradientRadius="8">
                <item android:color="#FFCCFF90" android:offset="0.0"/>
                <item android:color="#FF64DD17" android:offset="0.7"/>
                <item android:color="#001B5E20" android:offset="1.0"/>
            </gradient>
        </aapt:attr>
    </path>

    <!-- Tip 6: Bottom Right -->
    <path android:pathData="M65,62 C63,62 61,65 63,68 C65,71 69,71 71,68 C73,65 71,62 68,62 Z">
        <aapt:attr name="android:fillColor">
            <gradient android:type="radial" android:centerX="67" android:centerY="65" android:gradientRadius="8">
                <item android:color="#FFCCFF90" android:offset="0.0"/>
                <item android:color="#FF64DD17" android:offset="0.7"/>
                <item android:color="#001B5E20" android:offset="1.0"/>
            </gradient>
        </aapt:attr>
    </path>

    <!-- Tentacle Connections (Translucent) -->
    <path android:pathData="M52,28 Q52,45 52,55 M32,38 Q40,45 52,55 M77,41 Q65,48 52,55 M37,68 Q45,62 52,55 M67,65 Q60,60 52,55"
          android:strokeWidth="4"
          android:strokeAlpha="0.3">
        <aapt:attr name="android:strokeColor">
            <gradient android:type="linear" android:startX="52" android:startY="28" android:endX="52" android:endY="55">
                <item android:color="#FF7B1FA2" android:offset="0.0"/>
                <item android:color="#FF4A148C" android:offset="1.0"/>
            </gradient>
        </aapt:attr>
    </path>

</vector>
